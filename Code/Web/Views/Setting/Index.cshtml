@using Domain
@model System.Collections.Generic.List<Domain.Setting>

@{
    ViewBag.Title = "System Settings";
}

<h2>System Settings</h2>

<table>
    <tr>
        <th style="width: 200px;">Setting</th>
        <th>Value</th>
        <th></th>
    </tr>
    @foreach(Setting setting in Model)
    {
        <tr>
            <td>@setting.Description</td>
            <td>@setting.Value</td>
            <td>@Html.ActionLink("Edit", "Edit", "Setting", new {id = setting.Id}, null)</td>
        </tr>
    }
</table>